#include <iostream>
#include <string>
#include <stack>

using namespace std;

	
int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
	
	string str;
	cin >>str;

	string answer = "";
	for(int i = str.size()-1; i>=0; i--){
		if(str[i] == '(') {
			int n = str[--i] - '0';
			
			string tmp = answer;
			if(n==0) answer = "";
			else for(int j=1;j<n;j++) answer += tmp;
		}
		else if(str[i] != ')') answer.insert(0, 1, str[i]);
	}
	
	cout << answer.size();
	
	return 0;
}
